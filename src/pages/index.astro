---
import Card from '../components/Card.astro';
import Intro from '../components/Intro.astro';
import Bento from '../components/Bento.astro';
import Layout from '../layouts/Layout.astro';
import Icon from '../components/Icon.astro';
import Contact from '../components/Contact.astro';
import Footer from '../components/Footer.astro';
---

<Layout>
    <main class="flex items-center justify-center flex-col font-mono text-white">
        <section class="md:w-[750px] md:flex-row md:pt-[20vh] pt-[5vh] flex flex-col-reverse w-auto text-white m-auto items-center  ">
            <Intro />
        </section>

        <section class="pt-[5vh]">
            <svg class="animate-bounce w-6 h-6 text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 14">
                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 1v12m0 0 4-4m-4 4L1 9"/>
            </svg>
        </section>

        <section class=" mt-[15vh]" id="experience">
            <Card>
                <article class="flex flex-col items-start justify-items-start">
            <span class="flex pl-6 pr-6 items-center"><img class="max-[425px]:max-w-[100px] max-[425px]:max-h-[25px] max-w-[150px] max-h-[38px]" src="/kanaicode_logo.webp" alt="Kanaicode logo"/> <h4 class="max-[425px]:text-[15px] opacity-90 font-bold"> - FullStack Developer</h4></span>
            <p class="opacity-90 pl-6 text-sm">June, 2022 - September, 2023</p>
            <ul class="list-disc pl-18 pt-4 opacity-90">
                <li>Develop <span class="text-yellow-500">high-quality</span>, responsive and <span class="text-yellow-500">user-friendly</span> web aplications.</li>
                <li><span class="text-yellow-500">Optimize</span> applications performance, size and scalability.</li>
                <li>Develop administration web apps (cms, dashboard, etc).</li>
                <li>Help <span class="text-yellow-500">Backend team</span> adding new endpoints and features.</li>
                <li>Review the code of new employees and <span class="text-yellow-500">help</span> them with their onbording process.</li>
            </ul>
        </article>
        
            </Card>
        </section>

        <section class="mt-[30vh]" id="projects">
            <Bento />
        </section>

        <section class="mt-[30vh]" id="stack">
            <h2 class="text-4xl text-center font-extrabold pb-10 opacity-90">Stack</h2>
